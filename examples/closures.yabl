
fun make_vec(x, y){
    var _x = x;
    var _y = y;
    
    fun getX(){ return _x; }
    fun setX(x){ _x = x; }
    fun getY(){ return _y; }
    fun setY(y){ _y = y; }
    
    fun add(){ return _x + _y; }

    return [getX, getY, setX, setY, add];
}

var vec = make_vec(20, 10);
print vec[0]();
print vec[1]();
print vec[4]();


