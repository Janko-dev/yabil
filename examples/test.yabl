class Vec { 
    init(x, y){
        this.x = x;
        this.y = y;
    }

    sum(){
        return this.x + this.y;
    }

    add(vec){
        return Vec(this.x + vec.x, this.y + vec.y);
    }
}

var p = Vec(2, 6);
var q = Vec(10, -3);

print Vec(p.add(q).add(Vec(10, 20)).sum(), Vec(2, 3).sum()).sum();